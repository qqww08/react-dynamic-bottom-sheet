{
  "name": "react-dynamic-bottom-sheet",
  "version": "2.0.0",
  "description": "react-dynamic-bottom-sheet",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/qqww08/react-dynamic-bottom-sheet",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:dist && tsup src/index.ts --format esm,cjs --dts --external react --external react-dom",
    "watch": "tsup src/index.ts --watch",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "clean:dist": "rm -rf dist",
    "start": "parcel index.html",
    "docs:watch": "cd docs && sudo npm link ../ && npm run dev",
    "docs:build": "cd docs && npm run build",
    "serve-storybook": "cd docs && serve storybook-static"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "DaeHyun Shin",
    "email": "qqwwee08@naver.com"
  },
  "keywords": [
    "react",
    "paymnet",
    "password",
    "keypad"
  ],
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.3.23",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-scripts": "5.0.1",
    "semantic-release": "^19.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0 || ^18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qqww08/react-dynamic-bottom-sheet.git"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
