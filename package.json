{
  "name": "react-dynamic-bottom-sheet",
  "version": "2.0.3-1",
  "description": "react-dynamic-bottom-sheet",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/qqww08/react-dynamic-bottom-sheet",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:dist && tsup",
    "watch": "tsup src/index.ts --watch",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "clean:dist": "rm -rf dist",
    "start": "parcel index.html",
    "docs:watch": "cd docs && sudo npm link ../ && npm run dev",
    "docs:build": "cd docs && npm run build",
    "serve-storybook": "cd docs && serve storybook-static"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "DaeHyun Shin",
    "email": "qqwwee08@naver.com"
  },
  "keywords": [
    "react",
    "paymnet",
    "password",
    "keypad"
  ],
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-transition-group": "^4.4.5",
    "scheduler": "^0.23.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@parcel/core": "^2.11.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.4.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-is": "^18.2.4",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^18.2.0",
    "react-scripts": "5.0.1",
    "semantic-release": "^23.0.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qqww08/react-dynamic-bottom-sheet.git"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
